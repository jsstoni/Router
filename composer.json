{
  "name": "jsstoni/router",
  "description": "Router HTTP",
  "keywords": [
    "router",
    "routing",
    "route",
    "php",
    "url-handling",
    "input-handler",
    "routing-engine",
    "request-handler"
  ],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "jsstoni",
      "email": "jsstoniha@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "firebase/php-jwt": "^6.9",
    "league/plates": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Route\\": "src/"
    }
  },
  "scripts": {
    "phpcs": "@php vendor/bin/php-cs-fixer fix --dry-run --diff",
    "phpcs:fix": "@php vendor/bin/php-cs-fixer fix"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.41"
  }
}
